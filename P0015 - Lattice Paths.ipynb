   "source": [
    "N = 20\n",
    "\n",
    "grid = [[0 if i < N else 1 for i in range(N+1)] if j < N else [1 for _ in range(N+1)] for j in range(N+1)]\n",
    "\n",
    "i, j = N-1, N-1\n",
    "\n",
    "while i >= 0:\n",
    "    while j >= 0:\n",
    "        grid[i][j] = grid[i+1][j] + grid[i][j+1]\n",
    "        j -= 1\n",
    "    j = N-1\n",
    "    i -= 1\n",
    "    \n",
    "print(grid[0][0])"
   ]
