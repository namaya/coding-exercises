    "def is_prime(n):\n",
    "    for i in range(2, int(n**(1/2)) + 1):\n",
    "        if n % i == 0:\n",
    "            return False\n",
    "    return True\n",
    "\n",
    "N = 2000000\n",
    "\n",
    "sum_ = 2\n",
    "for i in range(3, N+1, 2):\n",
    "    if is_prime(i):\n",
    "        sum_ += i\n",
    "        \n",
    "print(sum_)"
   ]
